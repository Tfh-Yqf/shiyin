
.tran_button{
	width: 80%;
	height: 40px;
	border: thin solid #fff;
	border-radius: 20px;
	text-align: center;
	display: flex;
	flex-direction: column;
	justify-content: center;
	flex-shrink: 0;
}
.white_button{
	width: 80%;
	height: 40px;
	border: thin solid #fff;
	border-radius: 20px;
	text-align: center;
	display: flex;
	flex-direction: column;
	justify-content: center;
	background-color: #fff;
	flex-shrink: 0;
}
.box_login{
	background: linear-gradient(to top right,#99c5ff,#59f7ad);
	border-radius: 10px;
	display: flex;
	flex-direction: column;
	justify-content: space-around;
	align-items: center;
	height: 700rpx;
}

